<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/grid"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:columnCount="3">

        <ImageView
            android:id="@+id/i1_id"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/dos"
            tools:srcCompat="@tools:sample/backgrounds/scenic"/>
        <ImageView
            android:id="@+id/i2_id"
            android:src="@drawable/CB3DA6374"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_width="137dp"
            android:layout_height="137dp"/>
        <ImageView
            android:id="@+id/i3_id"
            android:src="@drawable/descarga"
            android:layout_column="2"
            android:layout_row="0"
            android:layout_width="137dp"
            android:layout_height="137dp"/>

        <ImageView
            android:id="@+id/i4_id"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:layout_row="1"
            android:layout_column="0"
            android:src="@drawable/android-2-3-gingerbread-logo"
            tools:srcCompat="@tools:sample/backgrounds/scenic" />

        <ImageView
            android:id="@+id/i5_id"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:layout_row="1"
            android:layout_column="1"
            android:src="@drawable/Uganda"
            tools:srcCompat="@tools:sample/backgrounds/scenic" />

        <ImageView
            android:id="@+id/i6_id"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:layout_row="1"
            android:layout_column="2"
            android:src="@drawable/Screenshot_20181013-133015_2-750x470"
            tools:srcCompat="@tools:sample/backgrounds/scenic" />
        <ImageView
            android:id="@+id/i7_id"
            android:src="@drawable/Cover_678x452"
            android:layout_column="0"
            android:layout_row="2"
            android:layout_width="137dp"
            android:layout_height="137dp"/>
        <ImageView
            android:id="@+id/i8_id"
            android:src="@drawable/Android_Kitkat"
            android:layout_column="1"
            android:layout_row="2"
            android:layout_width="137dp"
            android:layout_height="137dp"/>

        <ImageView
            android:id="@+id/i9_id"
            android:src="@drawable/logo-Android-Andy"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:layout_row="2"
            android:layout_column="2"
            tools:srcCompat="@tools:sample/backgrounds/scenic" />
    </GridLayout>

</android.support.constraint.ConstraintLayout>